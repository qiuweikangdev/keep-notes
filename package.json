{
  "name": "keep-notes",
  "type": "module",
  "version": "1.0.0",
  "description": "Markdown编辑器笔记软件",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "nodemon --exec electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit": "git-cz",
    "update": "pnpm dlx taze -w"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@milkdown/core": "^7.3.5",
    "@milkdown/ctx": "^7.3.5",
    "@milkdown/plugin-block": "^7.3.5",
    "@milkdown/plugin-clipboard": "^7.3.5",
    "@milkdown/plugin-cursor": "^7.3.5",
    "@milkdown/plugin-emoji": "^7.3.5",
    "@milkdown/plugin-history": "^7.3.5",
    "@milkdown/plugin-indent": "^7.3.5",
    "@milkdown/plugin-listener": "^7.3.5",
    "@milkdown/plugin-prism": "^7.3.5",
    "@milkdown/plugin-slash": "^7.3.5",
    "@milkdown/plugin-tooltip": "^7.3.5",
    "@milkdown/plugin-trailing": "^7.3.5",
    "@milkdown/plugin-upload": "^7.3.5",
    "@milkdown/preset-commonmark": "^7.3.5",
    "@milkdown/preset-gfm": "^7.3.5",
    "@milkdown/prose": "^7.3.5",
    "@milkdown/theme-nord": "^7.3.5",
    "@milkdown/transformer": "^7.3.5",
    "@milkdown/utils": "^7.3.5",
    "@milkdown/vue": "^7.3.5",
    "@prosemirror-adapter/vue": "^0.2.6",
    "ant-design-vue": "^4.1.2",
    "electron-updater": "^6.1.8",
    "prism-themes": "^1.9.0",
    "splitpanes": "^3.1.5",
    "vue-router": "4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^18.19.18",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.17",
    "commitizen": "^4.3.0",
    "cz-git": "^1.8.0",
    "electron": "^28.2.4",
    "electron-builder": "^24.12.0",
    "electron-vite": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-plugin-vue": "^9.22.0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "tailwind-nord": "^1.3.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.4",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.4.19",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
