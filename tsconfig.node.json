{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "compilerOptions": {
    "composite": true,
    "baseUrl": "./",
    "paths": {
      "@common/*": [
        "src/common/*"
      ]
    },
    "types": [
      "electron-vite/node"
    ]
  },
  "include": [
    "electron.vite.config.*",
    "src/main/**/*",
    "src/main/*",
    "src/preload/*",
    "src/common/types/*.d.ts",
    "src/common/**/*",
    "src/common/*"
  ]
}
